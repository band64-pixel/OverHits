    Public Shared Function ExtractPlayerAsRed(player As Bitmap) As Bitmap
        Dim Finalbit As New Bitmap(player.Width, player.Height, PixelFormat.Format32bppRgb)

        For x As Integer = 0 To player.Width - 1
            For y As Integer = 0 To player.Height - 1

                Finalbit.SetPixel(x, y, Color.White)

                Dim col As Color = player.GetPixel(x, y)
                If IsRed(col, HPRange.r200) Then
                    Finalbit.SetPixel(x, y, col)
                End If

            Next
        Next

        Return Finalbit
    End Function
